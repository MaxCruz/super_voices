<p id="notice"><%= notice %></p>

<h1>Voices</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>Last name</th>
      <th>Done</th>
      <th>Message</th>
      <th>Source url</th>
      <th>Destination url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voices.each do |voice| %>
      <tr>
        <td><%= voice.email %></td>
        <td><%= voice.name %></td>
        <td><%= voice.last_name %></td>
        <td><%= voice.done %></td>
        <td><%= voice.message %></td>
        <td><%= voice.source_url %></td>
        <td><%= voice.destination_url %></td>
        <td><%= link_to 'Show', voice %></td>
        <td><%= link_to 'Edit', edit_voice_path(voice) %></td>
        <td><%= link_to 'Destroy', voice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voice', new_voice_path %>
